{
	"name": "dynamic-labels",
	"description": "The built-in 'Labels' Display, but get the label component's props from the chosen item rather than hard coded by column data.",
	"icon": "extension",
	"version": "1.0.5",
	"repository": {
		"directory": "https://github.com/gla23/dynamic-labels"
	},
	"homepage": "https://github.com/gla23/dynamic-labels",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-display"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "display",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^11.11.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "16.0.1",
		"typescript": "^5.9.2",
		"vue": "^3.5.21"
	}
}